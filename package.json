{
  "name": "stageup-core",
  "version": "0.0.4",
  "license": "MIT",
  "scripts": {
    "uml": "ts-node --project tools/tsconfig.tools.json tools/scripts/generate-uml",
    "runner": "cross-env NODE_ENV=development nx serve runner --configuration=development",
    "runner:testing": "cross-env NODE_ENV=testing nx serve runner --configuration=testing",
    "runner:staging": "cross-env NODE_ENV=staging nx serve runner --configuration=staging",
    "runner:production": "cross-env NODE_ENV=production nx serve runner --configuration=production",
    "build:runner:staging": "cross-env NODE_ENV=staging nx build runner --configuration=staging --generatePackageJson",
    "build:runner:production": "cross-env NODE_ENV=production nx build runner --configuration=production --generatePackageJson",
    "backend": "cross-env NODE_ENV=development nx serve backend --configuration=development",
    "backend:testing": "cross-env NODE_ENV=testing nx serve backend --configuration=testing",
    "backend:staging": "cross-env NODE_ENV=staging nx serve backend --configuration=staging",
    "backend:production": "cross-env NODE_ENV=production nx serve backend --configuration=production",
    "frontend:setenv": "ts-node ./apps/frontend/set-env.ts",
    "frontend": "npm run frontend:setenv -- --env=development && nx serve frontend --configuration=development",
    "frontend:staging": "npm run frontend:setenv -- --env=staging && nx serve frontend --configuration=staging",
    "frontend:production": "npm run frontend:setenv -- --env=production && nx serve frontend --configuration=production",
    "api-tests": "nx test api-tests --watch",
    "build:api-tests": "echo 'Not yet implemented'",
    "build:backend:staging": "cross-env NODE_ENV=staging nx build backend --configuration=staging --generatePackageJson",
    "build:backend:production": "cross-env NODE_ENV=production nx build backend --configuration=production --generatePackageJson",
    "build:frontend:staging": "npm run frontend:setenv -- --env=staging && nx build frontend --configuration=staging",
    "build:frontend:production": "npm run frontend:setenv -- --env=production && nx build frontend --configuration=production",
    "build:staging": "clear && npm run frontend:setenv -- --env=staging && nx run-many --all --target=build --parallel --configuration=staging",
    "build:production": "clear && npm run frontend:setenv -- --env=production && nx run-many --all --target=build --parallel --configuration=production",
    "lint": "nx workspace-lint && nx lint",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "ng-postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "bundle-report-frontend": "nx build frontend --prod --stats-json && webpack-bundle-analyzer dist/apps/frontend/stats.json",
    "release": "shipjs prepare"
  },
